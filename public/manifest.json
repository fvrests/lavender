{
    "name": "lavender | new tab",
    "short_name": "lavender",
    "version": "1.1",
    "description": "a soft, minimal new tab for your browser",
    "permissions": [
        "storage",
        "geolocation"
    ],
    "action": {
        "default_icon": {
            "16": "16.png",
            "32": "32.png",
            "48": "48.png",
            "128": "128.png"
        }
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "manifest_version": 3
}