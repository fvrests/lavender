{
  "name": "lavender | new tab",
  "version": "1.0",
  "description": "A clean, customizable new tab experience",
  "permissions": ["storage", "geolocation"],
  "page_action": {
    "default_icon": {
      "16": "/assets/icons/lavender-favicon-16@2x.png",
      "32": "/assets/icons/lavender-favicon-32@2x.png",
      "48": "/assets/icons/lavender-favicon-48@2x.png",
      "128": "/assets/icons/lavender-favicon-128@2x.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 2
}
